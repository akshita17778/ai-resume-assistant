# Example environment variables for AI Resume Assistant
# Choose provider: 'openai' or 'anthropic' (for Claude).
PROVIDER=openai

# OpenAI credentials
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini

# Anthropic credentials (if you want to use Claude)
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_MODEL=claude-haiku-4.5

# Flask
FLASK_APP=app.py
FLASK_ENV=development
PORT=5000
